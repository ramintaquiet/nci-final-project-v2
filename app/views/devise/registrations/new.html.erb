<div class="separator">
<div class="container1">

<h1>Register <img src="/other/logintr.png" width="85" height="75" alt=""> here
</h1>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>

  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :password %>
    <% if @minimum_password_length %>
    <em>(min <%= @minimum_password_length %> characters)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "new-password" %>
  </div>

  <div class="field">
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
  </div>



  
  <div class="field">
  <%= f.label :name %><br />
  <%= f.text_field :name %>
</div>

<div class="field">
  <%= f.label :phone %><br />
  <%= f.text_field :phone %>
</div>

<div class="field">
<%= f.label :town %><br />
<%= f.text_field :town %>
</div>

<div class="field">
<%= f.label :county %><br />
<%= f.text_field :county %>
</div>

<div class="text_area" rows="16">
<%= f.label :info %><br />
<%= f.text_area :info %>
</div>
</br>




  <div class="actions">
    <%= f.submit "Sign up", :class => "btn btn-secondary" %>
  </div>
<% end %>




</div>
</div>

<br>